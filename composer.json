{
    "name": "s1syphos/kirby3-gesetze",
    "description": "Linking german legal norms for Kirby v3, dependency-free & GDPR-friendly",
    "type": "kirby-plugin",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": ["getkirby", "kirby", "kirby3", "kirby-plugin", "api", "law", "laws", "legal", "legal tech", "gesetze", "gesetze-im-internet.de", "dejure", "dejure.org", "buzer", "buzer.de", "lexparency", "lexparency"],
    "scripts": {
        "cs-dry": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix"
    },
    "authors": [
        {
            "name": "Martin Folkers",
            "email": "hello@twobrain.io",
            "homepage": "https://twobrain.io",
            "role": "Project Author"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.1",
        "s1syphos/php-gesetze": "^0.4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "config": {
        "optimize-autoloader": true
    }
}
